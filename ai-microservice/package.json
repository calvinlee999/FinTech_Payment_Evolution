{
  "name": "ai-inference-microservice",
  "version": "1.0.0",
  "description": "Scalable TypeScript/Node.js AI Inference Microservice with GPT-5 Mini",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "docker:build": "docker build -t ai-inference-service .",
    "docker:run": "docker run -p 3001:3001 --env-file .env ai-inference-service"
  },
  "dependencies": {
    "@azure/identity": "^4.12.0",
    "@azure/openai": "^2.0.0",
    "@azure/core-auth": "^1.10.1",
    "applicationinsights": "^3.3.0",
    "express": "^4.19.2",
    "helmet": "^8.0.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "kafkajs": "^2.2.4",
    "redis": "^4.7.0",
    "zod": "^3.23.8",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^5.0.3",
    "openai": "^4.73.1",
    "winston": "^3.15.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.9.2",
    "tsx": "^4.19.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.15.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "openai",
    "azure",
    "gpt-5",
    "microservice",
    "typescript",
    "scalable"
  ]
}